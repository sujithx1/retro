document.addEventListener("DOMContentLoaded", function() {
    const removeAddressButtons = document.querySelectorAll(".remove-address");

    removeAddressButtons.forEach(button => {
        button.addEventListener("click", function(event) {
            event.preventDefault(); // Prevent default link behavior

            const addressId = this.getAttribute("data-address-id");

            // Show SweetAlert confirmation dialog
            Swal.fire({
                title: 'Are you sure?',
                text: 'You won\'t be able to revert this!',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, delete it!'
            }).then((result) => {
                if (result.isConfirmed) {
                    // Send AJAX request to remove the address
                    fetch(`/profile/address/remove?id=${addressId}`, {
                        method: 'POST', // or 'DELETE' depending on your backend setup
                    })
                    .then(response => {
                        if (!response.ok) {
                            throw new Error('Network response was not ok');
                        }

                        const addressCard = button.closest('.card'); // Find the closest ancestor element with class 'card'
                        if (addressCard) {
                            addressCard.remove(); // Remove the address card from the DOM
                        }
                        console.log('Address removed successfully');

                        // Optionally, show success message with SweetAlert
                        Swal.fire(
                            'Deleted!',
                            'The address has been removed.',
                            'success'
                        );
                    })
                    .catch(error => {
                        console.error('Error removing address:', error);

                        // Optionally, show error message with SweetAlert
                        Swal.fire(
                            'Error!',
                            'There was a problem removing the address.',
                            'error'
                        );
                    });
                }
            });
        });
    });
});



