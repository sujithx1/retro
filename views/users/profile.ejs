                if (statusElement) {
                    statusElement.textContent = 'Cancelled'; // Set the text to 'Cancelled'
                    statusElement.classList.add('text-danger'); // Optionally, add a class to style the text
                }   
                if (button) {
                    button.textContent = 'Cancelled'; // Set the button text to 'Cancelled'
                   
                    button.classList.add('btn-danger'); // Add the new class for the desired color
                    button.disabled = true; // Optionally, disable the button

                }


                


      } else {
                console.error('Error canceling order:', responseData.message);
            }
        } catch (error) {
            console.error('Network error:', error);
        }
    
    

        
          
        }
}




